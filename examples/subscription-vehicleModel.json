{
    "id": "urn:ngsi-ld:Subscription:ql-vehiclemodel",
    "type": "Subscription",
    "entities": [
        {
            "type": "VehicleModel"
        }
    ],
    "watchedAttributes": [
        "name",
        "brandName",
        "modelName",
        "manufacturerName",
        "vehicleType",
        "fuelType"
    ],
    "notification": {
        "format": "normalized",
        "attributes": [
            "name",
            "brandName",
            "modelName",
            "manufacturerName",
            "vehicleType",
            "fuelType",
            "tractionBatteryCapacity"
        ],
        "endpoint": {
            "uri": "http://quantumleap:8668/v2/notify",
            "accept": "application/json",
            "receiverInfo": [

  { "key": "Fiware-Service",     "value": "default" }
]
        }
    },
    "@context": [
        "https://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context.jsonld",
        "https://smart-data-models.github.io/dataModel.Transportation/context.jsonld"
    ]
}