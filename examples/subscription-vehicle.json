{
    "id": "urn:ngsi-ld:Subscription:ql-vehicle",
    "type": "Subscription",
    "entities": [
        {
            "type": "Vehicle"
        }
    ],
    "watchedAttributes": [
        "location",
        "mileageFromOdometer",
        "serviceStatus",
        "observationDateTime"
    ],
    "notification": {
        "format": "normalized",
        "attributes": [
            "location",
            "mileageFromOdometer",
            "serviceStatus",
            "observationDateTime",
            "vehiclePlateIdentifier",
            "fuelType",
            "vehicleType",
            "category",
            "address",
            "leasingInfo"
        ],
        "endpoint": {
            "uri": "http://quantumleap:8668/v2/notify",
            "accept": "application/json",
            "receiverInfo": [

  { "key": "Fiware-Service",     "value": "default" }
]
        }
    },
    "@context": [
        "https://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context.jsonld",
        "https://smart-data-models.github.io/dataModel.Transportation/context.jsonld"
    ]
}